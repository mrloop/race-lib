{
  "name": "race-lib",
  "version": "1.2.0",
  "description": "retrieve event data from britishcycling.org",
  "main": "dist/race-lib.cjs.js",
  "module": "dist/race-lib.esm.js",
  "browser": "dist/race-lib.umd.js",
  "repository": "https://github.com/mrloop/race-lib",
  "author": "mrloop <ewan@mrloop.com>",
  "license": "MIT",
  "keywords": [
    "isomorphic",
    "rollup",
    "britishcycling"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "ava",
    "pretest": "yarn run build"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "es6-promise": "^4.2",
    "event-target-shim": "^5.0",
    "serial-fetch": "1.0.0",
    "uri-js": "^4.2"
  },
  "devDependencies": {
    "@ava/babel": "1.0",
    "@babel/core": "7.15.8",
    "@babel/plugin-external-helpers": "^7.2",
    "@babel/preset-env": "7.15.8",
    "@babel/register": "7.15.3",
    "@rollup/plugin-babel": "^5.1",
    "@rollup/plugin-commonjs": "^14.0",
    "@rollup/plugin-node-resolve": "^8.4",
    "ava": "^3.10",
    "cheerio": "^1.0.0-rc.3",
    "node-fetch": "2.6.5",
    "qunit": "2.17.2",
    "race-fix": "^0.0.2",
    "regenerator-runtime": "0.13.9",
    "rollup": "2.58.0"
  },
  "ava": {
    "babel": true,
    "files": [
      "tests/*.js"
    ],
    "require": [
      "@babel/register",
      "regenerator-runtime"
    ]
  },
  "files": [
    "dist"
  ]
